@model IEnumerable<hirinchallenge.Models.category>
<div class="container">
    <div class="row">

        <div class="col-xl-12 col-lg-12 col-md-12 col-sm-12 col-12">
            @foreach (var item in Model)
            {
                <ul class="nav" style="float:left;padding-left:35px;">
                    <li>
                        @item.categoryname
                    </li>

                </ul>
            }
        </div>
    </div>


</div>